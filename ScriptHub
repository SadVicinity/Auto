CSimSection:NewToggle("Auto Hatch Santa Egg", "NIL", function(state)
    if state then
        getgenv().hatch = true;
	while wait() do
    if getgenv().hatch == true then
	game:service'ReplicatedStorage'.Events.Hatch:FireServer("Santa","Triple")
    end
    end
    else
       getgenv().hatch = false;
	while wait() do
   	 if getgenv().hatch == true then
	game:service'ReplicatedStorage'.Events.Hatch:FireServer("Santa","Triple")
   	      end
        end
    end
end)
